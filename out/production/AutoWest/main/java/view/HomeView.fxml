<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="450.0" prefWidth="544.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar fx:id="menu" prefHeight="25.0" prefWidth="870.0" VBox.vgrow="NEVER">
         <menus>
            <Menu fx:id="healthRecord" mnemonicParsing="false" text="Records">
               <items>
                  <MenuItem fx:id="createHealthRecord" mnemonicParsing="false" text="New record" />
                  <MenuItem fx:id="updateHealthRecord" mnemonicParsing="false" text="Edit an existing record" />
                  <MenuItem fx:id="deleteHealthRecord" mnemonicParsing="false" text="Delete an existing record" />
                  <MenuItem fx:id="viewHealthRecord" mnemonicParsing="false" text="View all records" />
               </items>
            </Menu>
            <Menu fx:id="profile" mnemonicParsing="false" text="Profile">
               <items>
                  <MenuItem fx:id="viewProfile" mnemonicParsing="false" text="View my profile" />
                  <MenuItem fx:id="updateProfile" mnemonicParsing="false" text="Update my profile" />
               </items>
            </Menu>
            <Menu fx:id="exportMenu" mnemonicParsing="false" text="Export">
              <items>
                <MenuItem fx:id="export" mnemonicParsing="false" text="Export to csv" />
              </items>
            </Menu>
            <Menu fx:id="aboutMenu" mnemonicParsing="false" text="About">
              <items>
                <MenuItem fx:id="about" mnemonicParsing="false" text="See About page" />
              </items>
            </Menu>
            <Menu fx:id="accountMenu" mnemonicParsing="false" text="Account">
              <items>
                <MenuItem fx:id="logOut" mnemonicParsing="false" text="Log out" />
              </items>
            </Menu>
         </menus>
      </MenuBar>
      <Label fx:id="welcomeName" prefHeight="43.0" prefWidth="544.0" text="Label" />
      <Button fx:id="testButton" mnemonicParsing="false" text="Test" />
   </children>
</VBox>
